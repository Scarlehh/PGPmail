{
	"manifest_version": 2,
	"name": "PGPmail",
	"description": "Implement OpenPGP for GMail",
	"version": "1.0",
	"icons": {
        "128": "icon128.png"
    },
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": [
			"encrypt.js"
		]
	},
	"content_scripts": [
		{
			"matches": ["*://mail.google.com/*"],
			"js": [
				"intercept.js"
			],
			"run_at": "document_start"
		}
	]
}
